{
  "extends": [
    "next/core-web-vitals",
    "airbnb-base"
  ],
  "rules": {
    "eol-last": "off",
    "comma-dangle": "off",
    "no-unused-vars": "warn",
    "linebreak-style": "off",
    "consistent-return": "off",
    "react/display-name": "off",
    "object-curly-newline": "off",
    "class-methods-use-this": "off",
    "implicit-arrow-linebreak": "off",
    "import/prefer-default-export": "off"
  }
}